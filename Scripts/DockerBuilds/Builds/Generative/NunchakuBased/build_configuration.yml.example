# https://docs.nvidia.com/deeplearning/frameworks/support-matrix/index.html
# We need CUDA 12.X for nunchaku
# We need a PyTorch container because a NVIDIA CUDA container doesn't have
# nvidia-smi.
# Container OS Ubuntu 24.04
# NVIDIA CUDA 12.9.1
# apex                       0.1
# Jinja2                     3.1.6
# numpy                      1.26.4
# torch                      2.8.0a0+5228986c39.nv25.6
# transformer_engine         2.4.0+3cd6870
docker_image_name: nunchaku-based:25.06-py3
base_image: nvcr.io/nvidia/pytorch:25.06-py3
build_args:
  # Look for your version here:
  # https://nunchaku.tech/docs/nunchaku/installation/installation.html#installing-nunchaku
  nunchaku_version: https://github.com/nunchaku-tech/nunchaku/releases/download/v1.0.2/nunchaku-1.0.2+torch2.8-cp312-cp312-linux_x86_64.whl
